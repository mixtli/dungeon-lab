{
  "name": "@dungeon-lab/web",
  "version": "0.1.0",
  "description": "Vue.js web frontend for Dungeon Lab",
  "type": "module",
  "scripts": {
    "dev": "vite -d --host 0.0.0.0",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src;  vue-tsc --noEmit",
    "eslint": "eslint src/**/*.{mts,vue}",
    "test": "vitest run",
    "check": "npm run build --workspace=@dungeon-lab/shared && npm run build --workspace=@dungeon-lab/client && vue-tsc --noEmit",
    "clean:test": "rimraf dist-test"
  },
  "dependencies": {
    "@dungeon-lab/plugin-dnd-5e-2024": "file:../plugins/dnd-5e-2024",
    "@dungeon-lab/shared": "file:../shared",
    "@dungeon-lab/shared-ui": "file:../shared-ui",
    "@headlessui/vue": "^1.7.16",
    "@heroicons/vue": "^2.2.0",
    "@mdi/font": "^7.4.47",
    "@svgdotjs/svg.js": "^3.2.4",
    "@svgdotjs/svg.panzoom.js": "^2.1.2",
    "@vueuse/core": "^10.7.0",
    "axios": "^1.8.1",
    "computeds": "latest",
    "handlebars": "^4.7.8",
    "konva": "^9.3.20",
    "path-browserify": "latest",
    "pinia": "^2.3.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "pixi.js": "^8.0.0",
    "socket.io-client": "^4.7.2",
    "ts-deepmerge": "^7.0.2",
    "vue": "^3.5.13",
    "vue-konva": "^3.2.1",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/handlebars": "^4.0.40",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "autoprefixer": "^10.4.16",
    "chalk": "^5.3.0",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.33.0",
    "globals": "^16.1.0",
    "jsdom": "^26.0.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.32.1",
    "vite": "^5.0.8",
    "vitest": "^1.0.4",
    "vue-tsc": "^2.0.6"
  }
}
