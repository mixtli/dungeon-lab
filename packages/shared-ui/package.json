{
  "name": "@dungeon-lab/shared-ui",
  "version": "0.1.0",
  "description": "Shared Vue.js UI components for Dungeon Lab plugins",
  "type": "module",
  "main": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./base/*": {
      "types": "./dist/base/*.d.mts",
      "import": "./dist/base/*.mjs"
    },
    "./types/*": {
      "types": "./dist/types/*.d.mts",
      "import": "./dist/types/*.mjs"
    },
    "./components/*": {
      "types": "./dist/components/*.d.mts",
      "import": "./dist/components/*.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "build": "npm run clean && tsc --project tsconfig.json",
    "dev": "tsc --project tsconfig.json --watch",
    "check": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "vitest run"
  },
  "dependencies": {
    "@dungeon-lab/shared": "file:../shared",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vue/typescript-plugin": "^2.0.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  }
}